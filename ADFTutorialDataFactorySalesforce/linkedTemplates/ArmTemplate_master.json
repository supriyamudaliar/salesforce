{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adftutorialdatafactorysalesforce"
		},
		"CRM_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CRM'"
		},
		"SalesConnectionFiles_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SalesConnectionFiles'"
		},
		"Employees_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "Employees.csv"
		},
		"Employees_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "adftutorial"
		},
		"OutputDataset_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"OutputDataset_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "adftutorial/output"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CRM_connectionString": {
						"value": "[parameters('CRM_connectionString')]"
					},
					"SalesConnectionFiles_connectionString": {
						"value": "[parameters('SalesConnectionFiles_connectionString')]"
					},
					"Employees_properties_typeProperties_fileName": {
						"value": "[parameters('Employees_properties_typeProperties_fileName')]"
					},
					"Employees_properties_typeProperties_folderPath": {
						"value": "[parameters('Employees_properties_typeProperties_folderPath')]"
					},
					"OutputDataset_properties_typeProperties_fileName": {
						"value": "[parameters('OutputDataset_properties_typeProperties_fileName')]"
					},
					"OutputDataset_properties_typeProperties_folderPath": {
						"value": "[parameters('OutputDataset_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}